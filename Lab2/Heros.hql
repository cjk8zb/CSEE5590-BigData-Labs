CREATE DATABASE hero;

USE hero;

CREATE TABLE superheros (
id INT,
name STRING,
gender STRING,
eye_color STRING,
race STRING,
hair_color STRING,
height FLOAT,
publisher STRING,
skin_color STRING,
alignment STRING,
weight FLOAT
) ROW FORMAT DELIMITED FIELDS TERMINATED BY ',';

DESCRIBE superheros;

