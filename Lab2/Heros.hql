CREATE TABLE finalsuper (
id INT,
name STRING,
gender STRING,
race STRING,
color struct<
  hair:STRING,
  eye:STRING,
  skin:STRING>,
size struct<
  height:FLOAT,
  weight:FLOAT>,
publisher STRING,
alignment STRING,
powers array<STRING>
) ROW FORMAT DELIMITED FIELDS TERMINATED BY '\t' collection items terminated by ',';


INSERT INTO TABLE finalsuper SELECT
superheros2.*,
superpowers2.powers
FROM superheros2 JOIN superpowers2 ON superheros2.name = superpowers2.name;


